<?xml version="1.0" encoding="utf-8" ?>
<scheduler>
  <schedulejob>
    <item sched_name="goEJob" job_name="messageSend">
      <sched_name>goEJob</sched_name>
      <job_name>messageSend</job_name>
      <WriteDBLog>true</WriteDBLog>
      <WriteTxtLog>true</WriteTxtLog>
      <AllowUsed>1</AllowUsed>
    </item>
  </schedulejob>

  <schedulejob_details>
    <item sched_name="goEJob" job_name="messageSend">
      <sched_name>goEJob</sched_name>
      <job_name>messageSend</job_name>
      <job_group>messageSendGroup</job_group>
      <description>信息发送作业</description>
      <job_class_name>Lcgoc.Scheduler.MessageSendJob</job_class_name>
      <is_durable>true</is_durable>
      <apiurl></apiurl>
      <ftpuser></ftpuser>
      <ftppassword></ftppassword>
      <cardidlist></cardidlist>
      <shopidlist></shopidlist>
      <companyID></companyID>
      <pagesCount></pagesCount>
      <StartTime></StartTime>
      <EndTime></EndTime>
      <extra1></extra1>
      <Threads></Threads>
      <MinThreadPages></MinThreadPages>
    </item>
  </schedulejob_details>

  <schedulejob_details_triggers>
    <item sched_name="goEJob" job_name="messageSend">
      <sched_name>goEJob</sched_name>
      <job_name>messageSend</job_name>
      <trigger_name>messageSendTrigger</trigger_name>
      <trigger_group>messageSendTriggerGrop</trigger_group>
      <job_group>messageSendGroup</job_group>
      <description></description>
      <cronexpression>0/10 * * * * ?</cronexpression>
      <repeat_count></repeat_count>
      <repeat_interval></repeat_interval>
      <StartTime></StartTime>
      <EndTime></EndTime>
      <trigger_type>cron</trigger_type>
    </item>
  </schedulejob_details_triggers>

</scheduler>
